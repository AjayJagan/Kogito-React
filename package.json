{
  "name": "react-kogito",
  "version": "1.0.0",
  "main": "index.js",
  "author": "ajay",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --hot --color --progress --info=true --config webpack.dev.js",
    "test": "./node_modules/.bin/jest",
    "test:coverage": "jest --coverage",
    "lint": "tslint -c ./tslint.json --project .",
    "format": "prettier --check --write ./src/**/*.{tsx,ts,js}",
    "build:bundle-profile": "webpack --config webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "yarn build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@patternfly/patternfly": "^2.27.1",
    "@patternfly/react-charts": "^4.7.9",
    "@patternfly/react-core": "^3.87.3",
    "@patternfly/react-icons": "^3.14.3",
    "@patternfly/react-styles": "^3.5.13",
    "@patternfly/react-table": "^2.17.5",
    "@types/react-scrollbar": "^0.4.10",
    "apollo-boost": "^0.4.4",
    "graphql": "^14.5.8",
    "react-apollo": "^3.1.3",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scrollbar": "^0.5.6"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.10.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.22",
    "@fortawesome/free-solid-svg-icons": "^5.10.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.19",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/webpack": "^4.39.1",
    "babel-jest": "^24.9.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "imagemin": "^7.0.0",
    "jest": "^24.9.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node": "^12.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.18.2",
    "raw-loader": "^3.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-test-renderer": "^16.10.2",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "svg-url-loader": "^3.0.0",
    "ts-loader": "^6.0.4",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.6.2",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test-shim.js",
      "<rootDir>/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-preprocessor.js",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "testMatch": [
      "**/tests/*.(ts|tsx|js)"
    ],
    "moduleNameMapper": {
      "\\.(scss|sass|css)$": "identity-obj-proxy"
    }
  }
}
